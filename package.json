  "author": "peterhs.kang@gmail.com",
  "bin": {
    "node-init": "./bin/node-init.js"
  },
  "devDependencies": {
    "got": "11.8.2",
    "@octokit/core": "3.5.1",
    "picomatch": "2.2.3",
    "yargs": "17.1.1",
    "webpack": "~5.50.0",
    "webpack-cli": "~4.8.0",
    "jest": "~27.0.6"
  },
  "nodeInitIgnore": [
    "**",
    ".*"
  ]
}
